@model List<StudentTeacherQnAPlatform.ViewModels.TeacherAnswerViewModel>

<h2>My Answers</h2>

<ul>
    @foreach (var answer in Model)
    {
        <li>
            Answered Question: <strong>@answer.QuestionTitle</strong> - @answer.AnswerContent
            <p><small>Posted on @answer.CreatedDate</small></p>
        </li>
    }
</ul>
